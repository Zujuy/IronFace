<div class="row container">
    <!-- Page Layout here -->
<div class="col s12">
    <div class="row">
      <div class="col s3" >
            <h5 class="center-align blue-text text-blue lighten-2">Adds</h5>
            <div class="collection">
                <a href="#!" class="collection-item grey-text">Find books</a>
                <a href="#!" class="collection-item grey-text">Find who helps</a>
            </div>
              <nav>
                <div class="nav-wrapper">
                <form>
                    <div class="input-field  blue lighten-2">
                    <input id="search" type="search" required>
                    <label class="label-icon" for="search"><i class="material-icons">search</i></label>
                    <i class="material-icons">close</i>
                    </div>
                </form>
                </div>
            </nav>
      </div>
    
{{#each events}}
      <div class="col s9">
        <div>
            <div class="card horizontal hoverable">
            <div class="card-image">
                {{!-- <img src=""> <!-- imagen de perfil de usuario --> --}}
            </div>
            <div class="card-stacked">
                <div class="card-content">
                <h3 class="header blue-text text-blue lighten-2">{{this.eventName}}</h3>
                <h4 class="grey-text">{{this.creatorName}} {{this.creatorlastName}}</h4>
                <h5>{{this.content}}</h5>
                <p> Date: {{this.date}}</p>
                <p>A las {{this.timeStart}} </p>
                </div>
            </div>
        </div>
      </div>
    {{/each}}


    </div>
</div>
</div>
</div>