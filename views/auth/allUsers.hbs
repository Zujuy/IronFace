<div class="row container">
    <!-- Page Layout here -->
<div class="col s12">
    <div class="row">
      <div class="col s3" >
          <h5 class="center-align blue-text text-blue lighten-2">Events</h5>
            <div class="collection">
                <a href="/{{user.role}}/event" class="collection-item grey-text">Crear eventos</a>
                <a href="/{{user.role}}/allevents" class="collection-item grey-text">Eventos</a>
            </div>
            <h5 class="center-align blue-text text-blue lighten-2">Adds</h5>
            <div class="collection">
                <a href="#!" class="collection-item grey-text">Find books</a>
                <a href="#!" class="collection-item grey-text">Find who helps</a>
            </div>
              <nav>
                <div class="nav-wrapper">
                <form>
                    <div class="input-field  blue lighten-2">
                    <input id="search" type="search" required>
                    <label class="label-icon" for="search"><i class="material-icons">search</i></label>
                    <i class="material-icons">close</i>
                    </div>
                </form>
                </div>
            </nav>
      </div>
    
      <div class="col s9">

            {{#each users}}
        <div>
            <div class="card horizontal hoverable">
            <div class="card-image">
                <img src="{{this.photoURL}}" name="photoURL" alt="profile_image" id="profilepic" class="responsive-img"> <!-- imagen de perfil de usuario -->
            </div>
            <div class="card-stacked">
                <div class="card-content">
                 <a type='submit' href="/{{../user.role}}/ironhacker/{{../user._id}}"  class="waves-effect  blue lighten-2 btn modal-trigger">{{this.username}} {{this.lastName}}</a>
                     <p>{{this.role}}</p></h5>
                     <p>{{this.bootCamp}}</p></h5>
                     <p>{{this.birthdate}}</p>
                       </div> 
            </div>
        </div>
      </div>
              {{/each}}

    </div>
</div>
</div>
</div>