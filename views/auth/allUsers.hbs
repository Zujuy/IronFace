<div class="row container">
    <h4 class="grey-text center-align">All the Ironhackers</h4>
    <!-- Page Layout here -->
<div class="col s12">
    <div class="row">
      <div class="col s3" >
          <h5 class="center-align blue-text text-blue lighten-2">Events</h5>
            <div class="collection">
                <a href="/{{user.role}}/event" class="collection-item grey-text">Add events</a>
                <a href="/{{user.role}}/allevents" class="collection-item grey-text">Events</a>
            </div>
            <h5 class="center-align blue-text text-blue lighten-2">Adds</h5>
            <div class="collection">
                <a href="https://barelo-app.herokuapp.com/" class="collection-item grey-text">Share your books with Barelo</a>
                <a href="https://hostinn.herokuapp.com/" class="collection-item grey-text">HostInn</a>
                <a href="https://vaquita-app.herokuapp.com/" class="collection-item grey-text">Vaquita</a>
                <a href="https://dynamic-course.herokuapp.com/" class="collection-item grey-text">Keep Learning with DYNAMIC</a>
                <a href="https://dynamic-course.herokuapp.com/" class="collection-item grey-text">Keep Learning with DYNAMIC</a>
            </div>
      </div>
    
      <div class="col s9">

            {{#each users}}
        <div>
            <div class="card horizontal hoverable">
            <div class="card-image">
                <img src="{{this.photoURL}}" name="photoURL" alt="profile_image" id="profilepic" class="responsive-img"> <!-- imagen de perfil de usuario -->
            </div>
            <div class="card-stacked">
                <div class="card-content">
                 <a class="blue-text blue lighten-2" href="/{{../user.role}}/ironhacker/{{../user._id}}" ><h5>{{this.username}} {{this.lastName}}</h5></a>
                     <p>{{this.role}}</p></h5>
                     <p>{{this.bootCamp}}</p></h5>
                     <p>{{this.birthdate}}</p>
                       </div> 
            </div>
        </div>
      </div>
              {{/each}}

    </div>
</div>
</div>
</div>